    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
  	<servlet-name>tt</servlet-name>
  	<jsp-file>/a7.jsp</jsp-file>
  	<init-param>
